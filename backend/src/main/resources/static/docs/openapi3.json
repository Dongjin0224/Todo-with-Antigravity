          "required" : false,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "200",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/api-todos1281101631"
                },
                "examples" : {
                  "todo-get-all" : {
                    "value" : "[{\"id\":1,\"text\":\"Test 1\",\"completed\":false,\"displayOrder\":0,\"createdAt\":null,\"updatedAt\":null},{\"id\":2,\"text\":\"Test 2\",\"completed\":true,\"displayOrder\":1,\"createdAt\":null,\"updatedAt\":null}]"
